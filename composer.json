{
    "name": "rjorel/website",
    "description": "Personal website",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "symfony/finder": "^4.2",
        "symfony/http-foundation": "^4.2",
        "twig/twig": "^2.8",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "serve": "php -S localhost:8000 -t www server.php"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
