{
    "name": "rjorel/website",
    "description": "Personal website",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "symfony/http-foundation": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "serve": "php -S localhost:8000 -t www"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
